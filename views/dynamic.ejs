<body>
    <!-- d-flex justify-content-center -->
    <div class="row mt-5 d-flex justify-content-center">
       
        
          <div class="col-lg-6 col-md-6 col-md-12 col-xs-12">
           
            <div class="row mb-2">

            <div class="col-xs-12 col-12 ">
            <select class="form-select" aria-label="Default select example" onchange="city_mun()" id="surveys">
              <option value="0">Select Survey ...</option>
              <%  data.forEach(function(e) { %>
                  
                <option value="<%= e.url_id %>"> <%= e.url_desc %> </option>

              <%  }); %>
               
              </select>
            </div>
       

            </div>


            <div class="row mb-2">

            <div class="col-xs-10 col-10 ">
            <select class="form-select" aria-label="Default select example"  id="city_mun">
                <option value="">Select City Municipality ...</option>
          
              
              </select>
            </div>
            <div class="col-xs-2 col-2 ">
                <input type="hidden" name="url_name" id="url_name" value="">
                <button type="button" class="btn btn-success" onclick="fetch_sum()"><i class="bi bi-cloud-download"></i></button>
            </div>
            </div>

            <div class="card shadow-lg">
